{{!-- homepage-interaction.hbs --}}

{{#> headerfooter-common }}

{{#*inline "body"}}
	{{#> block-entrance template="base" }}
	{{/block-entrance}}

	<div class="main-content">
	  <div class="container">
	    <h2 class="">Latest stories</h2>

	    <section class="latest-stories navigation-list row">
			{{#each posts}}
				{{>block-postpreview-simple columnwidth=6 post=.}}
			{{/each}}
	    </section>

	    <section class="block--featureposts">
			{{#each posts}}
				{{>block-article columnwidth=6 post=.}}
			{{/each}}
	    </section>
	  </div>
	</div>



	{{#> block-shortinfo template="base" }}
	{{/block-shortinfo}}
{{/inline}}

{{/headerfooter-common}}
